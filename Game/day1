secret_num = 59
print ( " Guess a number between 1 and 100")
num = input ()

while ( True ):
    
    if   int(num) > sceret_num :
        print ( " lower number " )
        num = input ()

    if int(num) < secret_num : 
        print ( " higher number" )
        num = input ()
        
    if int(num) == secret_num : 
        print ( "Same number ")
        break
