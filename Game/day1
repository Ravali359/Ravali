import random
num1= int(input ("enter a num")):
num2=int(input("enetr a num")):
num3 =random(radint(num1,num2)):
guess=int(input("guess the number")):

    
    if   num3 > guess :
        print ( " lower number " )
        

    if num3 < guess : 
